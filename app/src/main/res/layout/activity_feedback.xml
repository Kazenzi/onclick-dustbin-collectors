<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".feedback"
    android:background="@color/purple_200">

    <TextView
        android:id="@+id/mTvname"
        android:textColor="@color/text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:text="ONCLICK DUSTBIN COLLECTORS"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/medtfedName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:textColorHint="@color/hint"
        android:drawableStart="@drawable/ic_baseline_perrson"
        android:ems="10"
        android:hint="EnterName"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mTvname"
        android:drawableLeft="@drawable/ic_baseline_perrson" />

    <EditText
        android:id="@+id/medtfedphone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:drawableStart="@drawable/ic_baseline_phone_iphone_24"
        android:ems="10"
        android:textColorHint="@color/hint"
        android:hint="EnterNumber"
        android:inputType="phone"
        android:minHeight="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medtfedName"
        tools:ignore="UnknownIdInLayout"
        android:drawableLeft="@drawable/ic_baseline_phone_iphone_24" />

    <EditText
        android:id="@+id/medtfeedback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:layout_marginBottom="262dp"
        android:drawableStart="@drawable/ic_baseline_feedback_24"
        android:ems="10"
        android:gravity="start|top"
        android:hint="Feedback"
        android:textColorHint="@color/hint"
        android:inputType="textMultiLine"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medtfedphone"
        android:drawableLeft="@drawable/ic_baseline_feedback_24" />

    <Button
        android:id="@+id/mbtnfeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="54dp"
        android:background="@drawable/custom_btn"
        android:textColor="@color/text"
        android:text="SUBMIT"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medtfeedback" />


</androidx.constraintlayout.widget.ConstraintLayout>